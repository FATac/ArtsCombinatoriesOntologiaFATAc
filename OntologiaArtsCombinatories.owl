    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#A._Mendieta -->

    <Thing rdf:about="&dcterms;A._Mendieta">
        <rdf:type rdf:resource="&dcterms;Person"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <firstName rdf:datatype="&xsd;string">Anna</firstName>
        <lastName rdf:datatype="&xsd;string">Mendieta</lastName>
        <gender rdf:datatype="&xsd;string">female</gender>
        <dcterms:hasparticipedIn rdf:resource="&dcterms;Ana_Mendieta"/>
        <dcterms:bornIn rdf:resource="&dcterms;La_Habana"/>
        <dcterms:diedIn rdf:resource="&dcterms;Nova_York"/>
    </Thing>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#Ana_Mendieta -->

    <Thing rdf:about="&dcterms;Ana_Mendieta">
        <rdf:type rdf:resource="&dcterms;FrameActivity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <Abstract rdf:datatype="&xsd;string">La mort prematura d’Ana Mendieta i el caràcter efímer de les seves creacions han contribuït a que l’obra d’aquesta artista, poètica i crítica alhora, avui no es conegui prou. Nascuda a l’Havana el 1948 en el si d’una família benestant, va ser enviada als Estats Units amb la seva germana Raquelín, quan només tenia dotze anys, i criada en orfenats americans.</Abstract>
        <dcterms:hasLanguage rdf:resource="&dcterms;Catalan"/>
        <dcterms:hasLanguage rdf:resource="&dcterms;English"/>
        <dcterms:hasLanguage rdf:resource="&dcterms;Spanish"/>
    </Thing>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#Ana_Mendieta_expedient -->

    <Thing rdf:about="&dcterms;Ana_Mendieta_expedient">
        <rdf:type rdf:resource="&dcterms;CulturalManagement"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <dcterms:RegisterNumber rdf:datatype="&xsd;string">01</dcterms:RegisterNumber>
        <dcterms:Volum rdf:datatype="&xsd;string">214 u. (774 fulls); suport paper; suport fotogràfic</dcterms:Volum>
        <Abstract rdf:datatype="&xsd;string">Agrupa 19 subexpedients relatius a l&#39;organització de l&#39;exposició, l&#39;artista Ana Mendieta, les obres, el muntatge, els materials de difusió, la comunicació a la premsa, el catàleg i la itinerància de l&#39;exposició al Centro Galego de Arte Contemporánea.</Abstract>
        <dcterms:Holding rdf:datatype="&xsd;string">FAT</dcterms:Holding>
        <dcterms:Manuscript rdf:datatype="&xsd;string">Manuscrit ; Mecanografiat ; Imprès</dcterms:Manuscript>
        <dcterms:references rdf:resource="&dcterms;Ana_Mendieta"/>
        <dcterms:hasLanguage rdf:resource="&dcterms;Spanish"/>
    </Thing>
    


