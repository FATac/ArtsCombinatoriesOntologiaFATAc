<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY dcterms "http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:dcterms="http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#">
    <Ontology rdf:about="http://www.fundaciotapies.org/ontologies/2011/4/ac.owl"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#Abstract"/>
    <AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#firstName"/>
    <AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#gender"/>
    <AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#lastName"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#IsLocatedIn -->

    <ObjectProperty rdf:about="&dcterms;IsLocatedIn">
        <rdfs:domain rdf:resource="&dcterms;Location"/>
        <rdfs:range rdf:resource="&dcterms;Location"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#appliesOn -->

    <ObjectProperty rdf:about="&dcterms;appliesOn">
        <rdfs:range rdf:resource="&dcterms;Case-Files"/>
        <rdfs:range rdf:resource="&dcterms;Event"/>
        <rdfs:range rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;Role"/>
        <rdfs:range rdf:resource="&dcterms;SpecialObjects"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#bornIn -->

    <ObjectProperty rdf:about="&dcterms;bornIn">
        <rdfs:comment>Lloc geogràfic on ha nascut un persona.</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Location"/>
        <rdfs:domain rdf:resource="&dcterms;Person"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#carriedOutBy -->

    <ObjectProperty rdf:about="&dcterms;carriedOutBy">
        <rdfs:comment>Relació que defineix quina persona ha realitzat un Event, un Art Work, una Publicació o un Media.</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Agent"/>
        <rdfs:domain rdf:resource="&dcterms;Case-Files"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;SpecialObjects"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#comesFrom -->

    <ObjectProperty rdf:about="&dcterms;comesFrom">
        <rdfs:domain rdf:resource="&dcterms;Event"/>
        <rdfs:range rdf:resource="&dcterms;Event"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#consistOf -->

    <ObjectProperty rdf:about="&dcterms;consistOf">
        <rdfs:comment>This property identifies the instances of materials of Art Work is composed.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Art_Work"/>
        <rdfs:range rdf:resource="&dcterms;Material"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#continuesTo -->

    <ObjectProperty rdf:about="&dcterms;continuesTo">
        <rdfs:range rdf:resource="&dcterms;Event"/>
        <rdfs:domain rdf:resource="&dcterms;Event"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#createdIn -->

    <ObjectProperty rdf:about="&dcterms;createdIn">
        <rdfs:comment>Lloc geogràfic on s&#39;ha creta una obra d&#39;art, una publicació o un media. Location.</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Location"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;SpecialObjects"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#diedIn -->

    <ObjectProperty rdf:about="&dcterms;diedIn">
        <rdfs:comment>Lloc geogràfic on ha mort una persona.</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Location"/>
        <rdfs:domain rdf:resource="&dcterms;Person"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#fromsPartOf -->

    <ObjectProperty rdf:about="&dcterms;fromsPartOf">
        <inverseOf rdf:resource="&dcterms;isComposedOf"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasLanguage -->

    <ObjectProperty rdf:about="&dcterms;hasLanguage">
        <rdfs:comment>Aquesta relació defineix en quina llengua està un art work, d&#39;una publicació i d&#39;un media.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Case-Files"/>
        <rdfs:range rdf:resource="&dcterms;Language"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;SpecialObjects"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasMedia -->

    <ObjectProperty rdf:about="&dcterms;hasMedia">
        <rdfs:domain rdf:resource="&dcterms;Case-Files"/>
        <rdfs:range rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasNarrowerTerm -->

    <ObjectProperty rdf:about="&dcterms;hasNarrowerTerm">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasRepresentation -->

    <ObjectProperty rdf:about="&dcterms;hasRepresentation">
        <rdfs:comment>Aquesta relació vincula una imatge que és representativa d&#39;un esdeveniment i d&#39;una obra d&#39;art i d&#39;una publicació.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Art_Work"/>
        <rdfs:domain rdf:resource="&dcterms;Event"/>
        <rdfs:range rdf:resource="&dcterms;Image"/>
        <rdfs:domain rdf:resource="&dcterms;Publication"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasSpecificActivity -->

    <ObjectProperty rdf:about="&dcterms;hasSpecificActivity">
        <rdfs:comment>propiedad que representa las actividades específicas de una actividad marco, o una actividad específica que es subactividad de otra específica.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;FrameActivity"/>
        <rdfs:range rdf:resource="&dcterms;SpecificActivity"/>
        <rdfs:domain rdf:resource="&dcterms;SpecificActivity"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasSuperActivity -->

    <ObjectProperty rdf:about="&dcterms;hasSuperActivity">
        <rdfs:domain rdf:resource="&dcterms;FrameActivity"/>
        <inverseOf rdf:resource="&dcterms;superActivityOf"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasTranslation -->

    <ObjectProperty rdf:about="&dcterms;hasTranslation">
        <rdfs:comment>Aquesta relació defineix en quina llengua s&#39;ha traduït d&#39;un art work, una publicació i un media.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Audio"/>
        <rdfs:range rdf:resource="&dcterms;Audio"/>
        <rdfs:range rdf:resource="&dcterms;Publication"/>
        <rdfs:domain rdf:resource="&dcterms;Publication"/>
        <rdfs:domain rdf:resource="&dcterms;Text"/>
        <rdfs:range rdf:resource="&dcterms;Text"/>
        <rdfs:range rdf:resource="&dcterms;Video"/>
        <rdfs:domain rdf:resource="&dcterms;Video"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasType -->

    <ObjectProperty rdf:about="&dcterms;hasType">
        <rdfs:comment>Defineix la relació entre un objecte i la seva tipologia.
Ex. Una obra d&#39;art concreta és un quadre, una escultura, un performance, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Art_Work"/>
        <rdfs:range rdf:resource="&dcterms;KindArtWork"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasVersion -->

    <ObjectProperty rdf:about="&dcterms;hasVersion">
        <rdfs:comment>A related resource that is a version, edition, or adaptation of the described resource.</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;SpecialObjects"/>
        <rdfs:range rdf:resource="&dcterms;SpecialObjects"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#hasparticipedIn -->

    <ObjectProperty rdf:about="&dcterms;hasparticipedIn">
        <rdfs:range rdf:resource="&dcterms;Agent"/>
        <rdfs:domain rdf:resource="&dcterms;Event"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isAdministrativeOrganisation -->

    <ObjectProperty rdf:about="&dcterms;isAdministrativeOrganisation">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió administrativa de l&#39;entitat (Polítiques i procediments, circulars, organigrames, llocs de treball, gestió dels recursos humans, del patrimoni, etc).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Administration"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isArchiveServices -->

    <ObjectProperty rdf:about="&dcterms;isArchiveServices">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió dels serveis l&#39;Arxiu (Préstec, reprografia, consulta, difusió, etc.).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Administration"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isAssetManagment -->

    <ObjectProperty rdf:about="&dcterms;isAssetManagment">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió del patrimoni de l&#39;entitat (operacions de béns mobles, inventari de béns mobles, utilització de béns mobles, utilització de béns inmobñes, etc).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Administration"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isAuthors -->

    <ObjectProperty rdf:about="&dcterms;isAuthors">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa als autors que participen en una activitat (Event) (cartes, permisos, negociacions amb els autors que tan poden ser artistes, conferenciants, escriptors, etc).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;CulturalManagement"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isBookshopServices -->

    <ObjectProperty rdf:about="&dcterms;isBookshopServices">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió de la llibreria (inventaris, productes, correspondència, etc.).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Administration"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCase-Fileproducer -->

    <ObjectProperty rdf:about="&dcterms;isCase-Fileproducer">
        <rdfs:comment>Aquesta relació permet definir quines persones o departament de la Fundació són les productores d&#39;un expedient de l&#39;Arxiu.</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Agent"/>
        <rdfs:range rdf:resource="&dcterms;Case-Files"/>
        <rdfs:domain rdf:resource="&dcterms;Department"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCataloguingOriginalWork -->

    <ObjectProperty rdf:about="&dcterms;isCataloguingOriginalWork">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió de la catalogació d&#39;obra original d&#39;Antoni Tàpies (catàleg d&#39;obra original, expedient de catalogació, registre, informe de condicions, itineràncies, etc.).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCataloguingoftheGraphicWork -->

    <ObjectProperty rdf:about="&dcterms;isCataloguingoftheGraphicWork">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió de la catalogació d&#39;obra gràfica d&#39;Antoni Tàpies (catàleg d&#39;obra gràfica, expedient de catalogació, registre, informe de condicions, itineràncies, etc.).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCertificationofOriginalWork -->

    <ObjectProperty rdf:about="&dcterms;isCertificationofOriginalWork">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió de la certificació de l&#39;obra original d&#39;Antoni Tàpies (sol·licitud, fotografia de l&#39;obra, certificat, etc.).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCertificationoftheGraphicWork -->

    <ObjectProperty rdf:about="&dcterms;isCertificationoftheGraphicWork">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió de la certificació de l&#39;obra gràfica de l&#39;Antoni Tàpies (sol·licitud, fotografia de l&#39;obra, certificat, etc.).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCirculationSheet -->

    <ObjectProperty rdf:about="&dcterms;isCirculationSheet">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa al full de préstec de les obres d&#39;art d&#39;un esdeveniment)  (fulls de prèstec, correspondència amb els prestadors, etc.).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;CulturalManagement"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCollectedIn -->

    <ObjectProperty rdf:about="&dcterms;isCollectedIn">
        <rdfs:range rdf:resource="&dcterms;Collection"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;SpecialObjects"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isComposedOf -->

    <ObjectProperty rdf:about="&dcterms;isComposedOf">
        <rdfs:comment>This property describes specific components that are individually documented.</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Event"/>
        <rdfs:domain rdf:resource="&dcterms;Event"/>
        <rdfs:range rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:domain rdf:resource="&dcterms;SpecialObjects"/>
        <rdfs:range rdf:resource="&dcterms;SpecialObjects"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isComunicationostheEvent -->

    <ObjectProperty rdf:about="&dcterms;isComunicationostheEvent">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la comunicació d&#39;un esdeveniment (notes de premsa, gestió de la publicitat, elements de difusió, etc).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;CulturalManagement"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isConditionReport -->

    <ObjectProperty rdf:about="&dcterms;isConditionReport">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa all informes de condicions de les obres d&#39;art d&#39;un esdeveniment (informes de condició, fotografia de les onbres, correspondència amb restauradors, etc).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;CulturalManagement"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isConservationandRestoration -->

    <ObjectProperty rdf:about="&dcterms;isConservationandRestoration">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a l&#39;estat de conservació i restauració de l&#39;expedient d&#39;una obra de la col·lecció de la Fundació  (informes de restauració in de conservació, fotografies, etc).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isCopyrightProtection -->

    <ObjectProperty rdf:about="&dcterms;isCopyrightProtection">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la protecció de drets d&#39;autor de l&#39;obra de l&#39;Antoni Tàpies (permisos de reproducció de l&#39;obra, sol·licituds, gesions amb VEGAP, etc.).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isDescriptionandCondition -->

    <ObjectProperty rdf:about="&dcterms;isDescriptionandCondition">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa als informes de condició de les obres de la col·lecció de la Fundació i a la catalogació de les obres de l&#39;Antoni Tàpies  (informes de condició, etc.).</rdfs:comment>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:range rdf:resource="&dcterms;ProtectionPromotionAT"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isDescriptionofConditionEvent -->

    <ObjectProperty rdf:about="&dcterms;isDescriptionofConditionEvent">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa als informes de condició de les obres d&#39;un esdeveniment  (informes de condició, etc.).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;CulturalManagement"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isEducationalServices -->

    <ObjectProperty rdf:about="&dcterms;isEducationalServices">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió del Servei educatiu (visites guiades, organització d&#39;activitats, tallers, etc).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Case-Files"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isExternalRelations -->

    <ObjectProperty rdf:about="&dcterms;isExternalRelations">
        <rdfs:comment>Aquesta relació permet reuní tota la documentació de l&#39;arxiu de la Fundació relativa a la gestió de les relacions externes de l&#39;entitat (Relacions amb Administracions, Fundacions i altres centres d&#39;Art, Universitats, empreses, etc.).</rdfs:comment>
        <rdfs:range rdf:resource="&dcterms;Administration"/>
        <rdfs:domain rdf:resource="&dcterms;Media"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.fundaciotapies.org/ontologies/2011/4/ac.owl#isFilmedBy -->

    <ObjectProperty rdf:about="&dcterms;isFilmedBy">
